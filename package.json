{
  "name": "@vt7/el-input-currency",
  "description": "A Vue.js library input mask is built on element-ui and v-money",
  "version": "1.1.3",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve src/ElInputCurrency.vue",
    "build": "vue-cli-service build --target lib --name el-input-currency src/ElInputCurrency.vue",
    "lint": "vue-cli-service lint",
    "public": "npm publish --access public"
  },
  "main": "./dist/el-input-currency.common.js",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "vue",
    "element-ui",
    "input",
    "mask",
    "currency",
    "money"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "v-money": "^0.8.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "vue-template-compiler": "^2.6.11"
  }
}
